<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wzxc.kbengine.dao.ds1.SearchLogMapper">
    
    <resultMap type="com.wzxc.kbengine.dao.ds1.SearchLog" id="SearchLogResult">
        <result property="id"    column="id"    />
        <result property="logType"    column="log_type"    />
        <result property="paramtersIn"    column="paramters_in"    />
        <result property="searchType"    column="search_type"    />
        <result property="createTime"    column="create_time"    />
        <result property="creator"    column="creator"    />
        <result property="standby1"    column="standby1"    />
        <result property="standby2"    column="standby2"    />
        <result property="paramtersOut"    column="paramters_out"    />
    </resultMap>

    <sql id="selectSearchLogVo">
        select id, log_type, paramters_in, search_type, create_time, creator, standby1, standby2, paramters_out from search_log
    </sql>

    <select id="selectSearchLogList" parameterType="SearchLog" resultMap="SearchLogResult">
        <include refid="selectSearchLogVo"/>
        <where>  
            <if test="logType != null "> and log_type = #{logType}</if>
            <if test="paramtersIn != null  and paramtersIn != ''"> and paramters_in = #{paramtersIn}</if>
            <if test="searchType != null "> and search_type = #{searchType}</if>
            <if test="creator != null  and creator != ''"> and creator = #{creator}</if>
            <if test="standby1 != null  and standby1 != ''"> and standby1 = #{standby1}</if>
            <if test="standby2 != null  and standby2 != ''"> and standby2 = #{standby2}</if>
            <if test="paramtersOut != null  and paramtersOut != ''"> and paramters_out = #{paramtersOut}</if>
        </where>
    </select>
    
    <select id="selectSearchLogById" parameterType="Long" resultMap="SearchLogResult">
        <include refid="selectSearchLogVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertSearchLog" parameterType="SearchLog">
        insert into search_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="logType != null">log_type,</if>
            <if test="paramtersIn != null">paramters_in,</if>
            <if test="searchType != null">search_type,</if>
            <if test="createTime != null">create_time,</if>
            <if test="creator != null and creator != ''">creator,</if>
            <if test="standby1 != null">standby1,</if>
            <if test="standby2 != null">standby2,</if>
            <if test="paramtersOut != null">paramters_out,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="logType != null">#{logType},</if>
            <if test="paramtersIn != null">#{paramtersIn},</if>
            <if test="searchType != null">#{searchType},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="creator != null and creator != ''">#{creator},</if>
            <if test="standby1 != null">#{standby1},</if>
            <if test="standby2 != null">#{standby2},</if>
            <if test="paramtersOut != null">#{paramtersOut},</if>
         </trim>
    </insert>

    <update id="updateSearchLog" parameterType="SearchLog">
        update search_log
        <trim prefix="SET" suffixOverrides=",">
            <if test="logType != null">log_type = #{logType},</if>
            <if test="paramtersIn != null">paramters_in = #{paramtersIn},</if>
            <if test="searchType != null">search_type = #{searchType},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="creator != null and creator != ''">creator = #{creator},</if>
            <if test="standby1 != null">standby1 = #{standby1},</if>
            <if test="standby2 != null">standby2 = #{standby2},</if>
            <if test="paramtersOut != null">paramters_out = #{paramtersOut},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteSearchLogById" parameterType="Long">
        delete from search_log where id = #{id}
    </delete>

    <delete id="deleteSearchLogByIds" parameterType="String">
        delete from search_log where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>